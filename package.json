{
  "name": "Departika-Boilerplate",
  "version": "5.0.0",
  "description": "Departika boilerplate with Parcel JS",
  "license": "GPL-2.0 AND MIT",
  "devDependencies": {
    "@juggle/resize-observer": "^2.5.0",
    "@uupaa/dynamic-import-polyfill": "^1.0.2",
    "autoprefixer": "^9.5.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.2.1",
    "dynamic-import-polyfill": "^0.1.1",
    "node-sass": "^4.9.0",
    "objectFitPolyfill": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-css-url-loader": "^1.0.5",
    "parcel-plugin-goodie-bag": "^1.0.1",
    "parcel-plugin-inlinesvg": "^0.1.0",
    "parcel-plugin-purifycss": "^1.0.5",
    "parcel-plugin-url-loader": "^1.2.6",
    "parcel-plugin-watch-reload": "^0.0.2",
    "sass": "^1.25.0"
  },
  "scripts": {
    "watch": "parcel watch ./assets/scripts/js/main.js --public-url ./",
    "dev": "parcel ./assets/scripts/js/main.js",
    "build": "parcel build ./assets/scripts/js/main.js --public-url ./"
  },
  "jshintConfig": {
    "globals": {
      "jQuery": true
    }
  },
  "watchreload": {
    "files": "**/*.php"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.3.5",
    "foundation-sites": "^6.5.3",
    "gsap": "^2.1.3",
    "jquery": "^3.3.1",
    "motion-ui": "^1.2.2",
    "parcel": "^1.12.4",
    "scrollmagic": "^2.0.7",
    "swiper": "^4.5.0"
  },
  "browserslist": "> 0.25%, IE 11"
}
